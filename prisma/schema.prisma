datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Currency {
  NOK
}

model Account {
  id        String   @id @default(cuid())
  name      String?
  balance   Float    @default(0)
  currency  Currency @default(NOK)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
